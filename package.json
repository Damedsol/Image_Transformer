{
  "name": "imagetransformer",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky",
    "lint": "eslint \"src/**/*.{ts,tsx}\" \"*.{js,ts}\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" \"*.{js,ts}\" --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,scss}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css,scss}\"",
    "lint-staged": "lint-staged",
    "docker:dev": "docker compose up -d",
    "docker:dev:down": "docker compose down",
    "docker:prod": "docker compose -f docker-compose.prod.yml up -d",
    "docker:prod:down": "docker compose -f docker-compose.prod.yml down",
    "docker:logs": "docker compose logs -f",
    "docker:prod:logs": "docker compose -f docker-compose.prod.yml logs -f",
    "docker:prune": "docker system prune -f",
    "docker:status": "docker compose ps",
    "docker:restart": "docker compose restart",
    "docker:frontend:logs": "docker compose logs -f frontend",
    "docker:backend:logs": "docker compose logs -f backend"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/js": "^9.25.0",
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "@typescript-eslint/parser": "^8.30.1",
    "eslint": "^9.25.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.1.3",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "prettier": "^3.2.5",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.1"
  }
}
